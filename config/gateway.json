{
  "host": "127.0.0.1",
  "port": 19233,
  "authMode": "local-trusted",
  "routingStrategy": "performance",
  "loadBalancingStrategy": "performance-based",
  "maxConcurrentServices": 50,
  "logLevel": "info",
  "enableHealthChecks": true,
  "healthCheckInterval": 30000,
  "requestTimeout": 30000,
  "maxRetries": 3,
  "enableMetrics": true,
  "metricsRetentionDays": 7,
  "enableCors": true,
  "corsOrigins": [
    "http://localhost:3000"
  ],
  "maxRequestSize": 10485760,
  "sandbox": {
    "profile": "default",
    "portable": {
      "enabled": true,
      "inheritSystemPath": false,
      "allowDangerousEnvOverride": false,
      "networkPolicy": "local-only"
    },
    "container": {
      "prefer": false,
      "requiredForUntrusted": false,
      "defaultNetwork": "none",
      "defaultReadonlyRootfs": true,
      "allowedVolumeRoots": [
        "../mcp-sandbox",
        "./data"
      ],
      "envSafePrefixes": [
        "PB_",
        "PBMCP_",
        "MCP_",
        "BRAVE_"
      ]
    }
  },
  "rateLimiting": {
    "enabled": false,
    "maxRequests": 100,
    "windowMs": 60000,
    "store": "memory"
  },
  "ai": {
    "provider": "none",
    "model": "",
    "endpoint": "",
    "timeoutMs": 30000,
    "streaming": true
  }
}
